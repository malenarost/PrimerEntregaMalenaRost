<div class='table-container'>
  <h1>Todos los productos</h1>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Price</th>
        <th>Thubmail</th>
        <th>Code</th>
        <th>Stock</th>
      </tr>
    </thead>
    <tbody>
      {{#each allProducts}}
        <tr>
          <td>{{this.title}}</td>
          <td>{{this.description}}</td>
          <td>{{this.price}}</td>
          <td>{{this.thubmail}}</td>
          <td>{{this.code}}</td>
          <td>{{this.stock}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>