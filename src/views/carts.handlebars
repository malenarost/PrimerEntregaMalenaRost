<div class='cart-container'>
  <h1>Cart</h1>
  {{#each productsMap}}
    <div class='product-container'>
      <h2>Product Id: {{id}}</h2>
      <h2>Title: {{this.title}}</h2>
      <h2>Description: {{this.description}}</h2>
      <h2>Price: {{this.price}}</h2>
      <h2>Quantity: {{quantity}}</h2>
    </div>
  {{/each}}
</div>